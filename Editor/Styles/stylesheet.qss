}

QPushButton {
	color:rgb(180,180,180);
	background-color:rgb(78,78,78);
	border:solid;
	border:0px;
	border-radius:2px;
	margin:0px;
	padding:0px;
	height:20px;
	width:100px;
	background: qlineargradient(x2:1, y2:2, x2:0, y2:2,
				stop:0 rgb(110,110,110), 
				stop:0.5 rgb(88,88,88), 
				stop:1 rgb(78,78,78));
}

QPushButton::hover {
background-color: rgb(95,95,95);
color:rgb(200,200,200);
background: qlineargradient(x2:1, y2:2, x2:0, y2:2,
			stop:0 rgb(120,120,120), 
			stop:0.5 rgb(100,100,100), 
			stop:1 rgb(100,100,100))
}

QPushButton::selected {
	border:0px solid rgb(97,172,236);
	background-color:rgb(97,172,236);
	color:black;
}

QPushButton::checked {
	border:0px solid rgb(97,172,236);
	background-color:rgb(97,172,236);
	color:black;
}

QPushButton::pressed {
	border:0px solid rgb(97,172,236);
	background: qlineargradient(x2:1, y2:2, x2:0, y2:2,
				stop:0 rgb(35,35,35), stop: 0.9 rgb(55,55,55), stop:1 rgb(78,78,78));
	color:rgb(166,166,166);
}

QPushButton::disabled {
	color:rgb(120,120,120);
	background-color: qlineargradient(x2:1, y2:2, x2:0, y2:2,
				stop:0 rgb(80,80,80), stop: 0.5 rgb(75,75,75), stop:1 rgb(75,75,75));
}

