	margin:0px;
}

QMenu {
	background-color: rgb(50,50,50);
	color: rgb(166,166,166);
	border:1px solid;
	border-color:rgb(97,172,236);
}

QMenu::item:selected {
	background-color:rgb(97,172,236);
	color:black;
}

QMenu:separator {
	background-color:rgb(66,66,66);
	height:1px;
}

QLabelSeparatorWidget {
	qproperty-lineColor: rgb(66,66,66);
}

QLabelSeparatorWidget QLabel{
	color: rgb(166,166,166);
	margin:2px;
}

/* BEGIN: TrackView - Batch RenderOutput */
QTextEdit#CVarsField {
	background-color: rgb(50,50,50);
	border-color:rgb(97,172,236);
}
/* END: TrackView - Batch RenderOutput */

/* BEGIN: TrackView - Sequences Dialog */
QListWidget#SequencesField {
	background-color: rgb(35,35,35);
	border-color:rgb(97,172,236);
}
/* END: TrackView - Sequences Dialog */

QTabWidget {
	color:rgb(167,167,167);
	border-bottom:0px solid rgb(97,172,236);
}

QTabBar {
	border-bottom:0px solid rgb(97,172,236);
}

QTabBar::tab {                              /* generic tabs */
	margin-left:2px;
	margin-top: 0px;
	margin-bottom: 2px;
	padding-bottom:0px;
	padding-top:0px;
	padding-right:4px;
	padding-left:4px;
	color:black;
	background: qlineargradient(x2:1, y2:1, x2:0, y2:1,
				stop:0 rgb(150,150,150), stop: 0.1 rgb(140,140,140), stop:1 rgb(150,150,150));
	border-radius:0px;
	height:20px;
	border-radius:2px;
}

QTabBar::tab::disabled {
	color:black;
	background-color:rgb(120,120,120);
}

QTabBar::tab:selected::disabled {
	color:black;
	background-color:rgb(140,140,140);
	border-top: 0px;
}

QTabBar::close-button  {
	image: url(icons:Window/Window_Close_Dark.ico);
	margin-top:-4px;
}


QTabBar::tab:first{
	margin-left:2px;
	border-top-left-radius: 2px;
	border-top-right-radius: 0px;
	border-bottom-left-radius: 2px;
	border-bottom-right-radius: 0px;
	margin-right:1px;
}

QTabBar::tab:last{
	border-top-left-radius: 0px;
	border-top-right-radius: 2px;
	border-bottom-left-radius: 0px;
	border-bottom-right-radius: 2px;
	margin-left:0px;
}

QTabBar::tab:middle{
	margin-left:0px;
	border-top-left-radius: 0px;
	border-top-right-radius: 0px;
	border-bottom-left-radius: 0px;
	border-bottom-right-radius: 0px;
	margin-right:1px;
}

QTabBar::tab:hover {
	color:black;
	background-color:rgb(180,180,180);
}

QTabBar::tab:selected {
	color:black;
	background-color:rgb(97,172,236);
	padding-bottom:0px;
}

QTabBar::tab:only-one{
	margin-left:2px;
	border-top-left-radius: 2px;
	border-top-right-radius: 2px;
	border-bottom-left-radius: 2px;
	border-bottom-right-radius: 2px;
}


QToolWindowArea QTabBar {						/* Docking tabs */
	border-bottom:0px solid rgb(97,172,236);
}

QToolWindowArea > QTabBar {
	background-color:rgb(35,35,35);
}

 QToolWindowArea > QTabBar::tab {
	background-color: rgb(35,35,35);               /*inactive tabs*/
	color:rgb(166,166,166);
	margin:0px;
	border:0px;
	padding-left:6px;
	min-height:23px;
	border-radius:0px;
	padding-bottom:0px;
	border-radius:0px;
}

QToolWindowArea > QTabBar::tab:hover {
	color:rgb(166,166,166);
	background-color:rgb(78,78,78);
	border: none;
	border-top-left-radius: 0px;
	border-top-right-radius: 0px;
	border-bottom-left-radius: 0px;
	border-bottom-right-radius: 0px;
	border-top:0px solid rgb(78,78,78);
	border-left:0px solid rgb(78,78,78);
	border-right:0px solid rgb(78,78,78);
}

QToolWindowArea > QTabBar::tab:first, QToolWindowArea > QTabBar::tab:middle, QToolWindowArea > QTabBar::tab:last{
	border-radius:0px;
	margin:0px;
	padding-bottom:0px;
	
}

QToolWindowArea > QTabBar::tab:selected {
	border-bottom-color: black;
	background-color: rgb(66,66,66);
	color: rgb(200,200,200);
	border: none;
	border-top-left-radius: 0px;
	border-top-right-radius: 0px;
	border-bottom:0px solid rgb(95,95,95);
	border-top:2px solid rgb(97,172,236);
	padding-top:-2px;
}

QTabWidget QMenuBar {
	background-color:rgb(66,66,66);
	padding-bottom:0px;
	margin-bottom:0px;
}

QTabWidget QMenuBar:item {
	color:rgb(140,140,140);
	margin-bottom:0px;
}

QTabWidget QMenuBar:item:selected {
	color:rgb(200,200,200);
	background-color:rgb(109,109,109);
	margin-bottom:0px;
}

QTabWidget QMenuBar:item:pressed {
	background: rgb(97,172,236);
	color:black;
	margin-bottom:0px;
}

QToolWindowArea > QTabBar > QToolButton { /* arrows on the right side of tabs when there are too many of them */
	background-color:rgb(50,50,50);
}

QToolWindowArea > QTabBar > QToolButton:hover { /* arrows on the right side of tabs when there are too many of them */
	background-color:rgb(50,50,50);
}

QScrollBar:vertical {
	background:transparent;
	width: 12px;
	margin: 0px 0px 0px 0px;
}

QScrollBar:horizontal {
	background:transparent;
	height: 12px;
	margin: 0px 0px 0px 0px;
}

QScrollBar::add-line:horizontal, QScrollBar::add-line:vertical {
	border: 0px;
	width: 0px;
	height: 0px;
}

QScrollBar::sub-line:horizontal, QScrollBar::sub-line:vertical {
	border: 0px;
	width: 0px;
	height: 0px;
}

QScrollBar::down-arrow, QScrollBar::up-arrow {
	border: 0px;
	width: 0px;
	height: 0px;
}

QScrollBar::handle:vertical {
	background-color:rgb(110,110,110);
	border-radius:2px;
	margin-left:2px;
	margin-right:0px;
	min-height:20px;
}

QScrollBar::handle:horizontal {
	background-color:rgb(110,110,110);
	border-radius:2px;
	margin-top:2px;
	margin-bottom:0px;
	min-width:20px;
}

QScrollBar::handle:hover {
	background-color:rgb(140,140,140);
	border-radius:2px;
}

QScrollBar::handle:pressed {
	background-color:rgb(97,172,236);
	border-radius:2px;
}

 QPropertyTree {
	color:rgb(210,210,210);
	background-color:rgb(66,66,66);         /* windows background */
	selection-background-color:rgb(97,172,236);
	border: none;
	padding:0px;
	margin:0px;
	font-size: 12px;
	qproperty-branchClosed: url(icons:Window/Collapse_Arrow_Right_Shifted_Tinted.ico);
