	qproperty-editsScale:1.4;
	qproperty-rowSpacing:1.1;
}

QPropertyGroupHeader {
	background: qlineargradient(x2:1, y2:2, x2:0, y2:2,
				stop:0 rgb(84,84,84), stop: 0.5 rgba(73,73,73,255), stop:0.0 rgba(78,78,78,255));
	margin: 0px -3px 0px -20px;
	margin-top:1px;
	margin-bottom:1px;
}

QPropertyGroupBox {
	margin: 1px 0px 0px 0px;
	background: qlineargradient(x1:1,
				stop:0.7 rgb(58,58,58), 
				stop:1 rgb(66,66,66)); 			
}

QPropertySplitter {
	background-color:rgb(73,73,73);
